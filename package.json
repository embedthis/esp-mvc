{
    "name": "esp-mvc",
    "description": "ESP MVC Framework",
    "version": "5.0.0-rc0",
    "keywords": [
        "esp",
    ],
    "dependencies": {
        "esp-server": "~1.0",
    },
    "dirs": {
        "?client": "client",
        "?paks": "client/paks"
    },
    "export": [
        "generate/**"
    ],
    "esp": {
        "?serverPrefix": "/do",
        "server": {
            "?database": "",
            "generate": {
                "?appweb": "esp-mvc/appweb.conf",
                "?migration": "esp-mvc/migration.c",
                "?module": "esp-mvc/src/app.c",
                "?controller": "esp-mvc/controller.c",
                "?controller-singleton": "esp-mvc/controller.c",
            }
        }
    }
}
